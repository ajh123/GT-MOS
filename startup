
local o = 1

while true do
 term.clear()
 term.setCursorPos(1,1)
 if o == 1 then
  print("Login <")
  print("Update")
  print("Advance Settings")
 elseif o == 2 then
  print("Login")
  print("Update <")
  print("Advance Settings")
 elseif o == 3 then
  print("Login")
  print("Update")
  print("Advance Settings <")
 end
 _, key = os.pullEvent("key")
 if key == keys.up and o > 1 then
  o = o - 1
 elseif key == keys.down and o < 3 then
  o = o + 1
 elseif key == keys.enter then
  break
 end
end

